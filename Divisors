import java.util.*;

class Solution {
    public List<Integer> divisors(int n) {
        List<Integer> result = new ArrayList<>();

        // Loop from 1 to n
        for (int i = 1; i <= n; i++) {
            if (n % i == 0) {
                result.add(i); // i is a divisor
            }
        }

        return result; // Already in sorted order
    }
}
